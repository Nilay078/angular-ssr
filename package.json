{
  "name": "the-secretariat-website",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "dev:ssr": "ng run the-secretariat-website:serve-ssr",
    "serve:ssr": "node dist/the-secretariat-website/server/main.js",
    "build:ssr": "ng build && ng run the-secretariat-website:server",
    "prerender": "ng run the-secretariat-website:prerender",
    "build:ssr-sandbox": "ng build --configuration sandbox && ng run the-secretariat-website:server:sandbox",
    "build:ssr-staging": "ng build --configuration staging && ng run the-secretariat-website:server:staging",
    "build:ssr-production": "ng build --configuration production && ng run the-secretariat-website:server:production"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/material": "^14.2.5",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/platform-server": "^14.2.0",
    "@angular/router": "^14.2.0",
    "@auth0/angular-jwt": "^5.0.2",
    "@ng-bootstrap/ng-bootstrap": "^12.0.0",
    "@nguniversal/express-engine": "^14.2.0",
    "@types/lodash": "^4.14.191",
    "bootstrap": "^5.1.3",
    "express": "^4.15.2",
    "mat-select-filter": "^2.4.1",
    "ng-navigator-share": "^1.0.4",
    "ng-otp-input": "^1.8.5",
    "ngx-cookie-service-ssr": "^16.0.0",
    "ngx-doc-viewer": "^14.0.0",
    "ngx-infinite-scroll": "^6.0.1",
    "ngx-skeleton-loader": "^6.0.0",
    "ngx-useful-swiper": "^10.0.1",
    "node-fetch": "^3.3.2",
    "rxjs": "~7.5.0",
    "ssr-window": "^4.0.2",
    "swiper": "^6.8.4",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.3",
    "@angular-eslint/builder": "14.1.2",
    "@angular-eslint/eslint-plugin": "14.1.2",
    "@angular-eslint/eslint-plugin-template": "14.1.2",
    "@angular-eslint/schematics": "14.1.2",
    "@angular-eslint/template-parser": "14.1.2",
    "@angular/cli": "~14.2.3",
    "@angular/compiler-cli": "^14.2.0",
    "@nguniversal/builders": "^14.2.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.0.0",
    "@types/node": "^14.15.0",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "prettier": "^2.7.1",
    "typescript": "~4.7.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,html}": [
      "prettier --write --config .prettierrc.json",
      "eslint --fix"
    ]
  }
}
